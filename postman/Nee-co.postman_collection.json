				{
					"name": "ユーザLIKE検索",
					"request": {
						"url": "{{host}}/users/search?str=田&college_ids=c",
						"method": "GET",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "Authorization",
